// stdout:
//   1
//   2
//   3

union List =
    | Cons {
        head: Int,
        tail: List,
    }
    | Nil;

val nil = Nil();

fun cons(head: Int, tail: List) -> List {
    Cons(.head = head, .tail = tail)
}

fun printInts(list: List) !Io -> Unit {
    if (list is Cons) {
        print!(intToString(list.head) + "\n");
        printInts!(list.tail);
    }
}

fun main() !Io -> Unit {
    val list = cons(1, cons(2, cons(3, nil)));
    printInts!(list);
}
