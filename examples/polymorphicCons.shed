// stdout:
//   1
//   2
//   3

union List[T] =
    | Cons[T] {
        head: T,
        tail: List[T],
    }
    | Nil;

val nil = Nil();

fun cons[T](head: T, tail: List[T]) -> List[T] {
    Cons(head=head, tail=tail)
}

fun printInts(list: List[Int]) !Io -> Unit {
    if (list is Cons[Int]) {
        print(intToString(list.head) + "\n");
        printInts(list.tail);
    }
}

fun main() !Io -> Unit {
    val list = cons(1, cons(2, cons(3, nil)));
    printInts(list);
}
